<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="iconTint">#FFFFFF</item>
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="DarkDialogTheme" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:background">#424242</item>
        <item name="android:textColorPrimary">#FFFFFFFF</item>
        <item name="textColorAlertDialogListItem">#FFFFFFFF</item>
        <item name="android:colorPrimary">?attr/colorPrimary</item>
        <item name="android:colorAccent">?attr/colorAccent</item>
        <item name="colorControlNormal">#B3FFFFFF</item>
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="colorControlHighlight">#B3FFFFFF</item>
        <item name="android:textColorHint">#B3FFFFFF</item>
    </style>

    <style name="DarkDialogThemeFullScreen">
        <item name="android:background">#424242</item>
        <item name="android:textColorPrimary">#FFFFFFFF</item>
        <item name="textColorAlertDialogListItem">#FFFFFFFF</item>
        <item name="android:colorPrimary">?attr/colorPrimary</item>
        <item name="android:colorAccent">?attr/colorAccent</item>
        <item name="colorControlNormal">#B3FFFFFF</item>
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="colorControlHighlight">#B3FFFFFF</item>
        <item name="android:textColorHint">#B3FFFFFF</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
    </style>

    <style name="LightDialogThemeFullScreen" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">false</item>
    </style>

    <style name="LightTheme">
        <item name="backgroundColor">#FAFAFA</item>
        <item name="cardColor">#FFFFFF</item>
        <item name="textColorPrimary">#212121</item>
        <item name="textColorSecondary">#757575</item>
        <item name="dividerColor">#BDBDBD</item>
        <item name="iconTint">#4A4A4A</item>
        <item name="colorControlNormal">#757575</item>
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="colorControlHighlight">#757575</item>
    </style>

    <style name="DarkTheme">
        <item name="backgroundColor">#303030</item>
        <item name="cardColor">#424242</item>
        <item name="textColorPrimary">#FFFFFFFF</item>
        <item name="textColorSecondary">#B3FFFFFF</item>
        <item name="dividerColor">#1FFFFFFF</item>
        <item name="iconTint">#FFFFFF</item>
        <item name="colorControlNormal">#B3FFFFFF</item>
        <item name="colorControlActivated">?attr/colorAccent</item>
        <item name="colorControlHighlight">#B3FFFFFF</item>
        <item name="android:textColorHint">#B3FFFFFF</item>
    </style>

